{
  "name": "Redirect qaru.site to stackoverflow",
  "version": "1.0",
  "description": "Redirect qaru.site to stackoverflow",
  "manifest_version": 2,
  "content_scripts": [ {
        "exclude_globs":    [  ],
        "include_globs":    [ "*" ],
        "js":               [ "qaru.block.user.js" ],
        "matches":          [ "https://*/*", "http://*/*" ],
        "run_at": "document_end"
    } ],
    "converted_from_user_script": true
}
